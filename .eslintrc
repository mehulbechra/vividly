{
	"extends":"airbnb",
	"env": {
		"browser": true,
		"node": true,
		"jquery": true
	},
	"rules": {
		"no-unused-vars": [1,{ "argsIgnorePattern": "err"}],
		"no-param-reassign":0,
		"no-underscore-dangle":0,
		"max-len":0,
		"linebreak-style": 0,
		"no-console": 0,
		"consistent-return":0
	},
	"plugins":["html"]
}